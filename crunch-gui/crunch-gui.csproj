    <RootNamespace>Crunch_GUI</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>Resources\CrunchGui.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\CrunchGUI.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\CrunchGui.ico" />
  </ItemGroup>

  <ItemGroup>
